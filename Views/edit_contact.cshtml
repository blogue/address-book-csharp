<h1>Edit Contact</h1>
<form action="/contacts/details/@Model.GetId()" method="post">
  <label>Name: </label>
  <input type="text" name="name" value="@Model.GetName()"> <br>
  <label>Address: </label>
  <input type="text" name="address" value="@Model.GetAddress()"> <br>
  <label>Phone: </label>
  <input type="text" name="phone" value="@Model.GetPhone()"> <br>
  <button type="submit" name="button">Edit</button>
</form>

<a href="/">Home</a>
<a href="/contacts">View Contacts</a>
